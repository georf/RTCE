package de.uni_rostock.rtce.log;

/**
 * A DebugElement containing a debug message
 * 
 * These messages are interesting only for developers.
 * 
 * @author Sebastian Gaul <sebastian@dev.mgvmedia.com>
 */
public class DebugMessage extends LogElement {

	public DebugMessage(String message) {
		super(message);
	}

	public String toString() {
		return "Debug: " + message;
	}
}
